# EditorConfig

# Top-most EditorConfig file
root = true

# General settings
[*]
end_of_line = lf
trim_trailing_whitespace = true
insert_final_newline = true
charset = utf-8
indent_style = space
indent_size = 4

# Matches the exact files either package.json or .travis.yml
[{*.yml, package.json}]
indent_style = space
indent_size = 2
